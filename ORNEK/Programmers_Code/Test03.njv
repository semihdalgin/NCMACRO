function Main() {
var SS,o;
  with (Netcad) {
    SS = NewSelectStatus();
    while (SelectObjectInstant('Doðru seç',1,MakeFilter(Array(_line,_pline)),SS)) {
      o = SS.objects(0);
      o.renk = Yellow;
      PutObject(SS.indexs(0), o);
      DrawObject(o,-1);
      o = null;
    }
    SS = null;
  }
}

